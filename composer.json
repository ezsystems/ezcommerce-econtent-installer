{
    "name": "ezsystems/ezcommerce-econtent-installer",
    "description": "Ibexa Commerce eContent data installer",
    "minimum-stability": "stable",
    "license": "proprietary",
    "type": "ibexa-bundle",
    "authors": [
        {
            "name": "Ibexa dev-team",
            "homepage": "https://github.com/ezsystems/ezcommerce/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ibexa\\Platform\\Bundle\\Commerce\\EContentInstaller\\": "src/bundle"
        }
    },
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "ezsystems/ezplatform-code-style": "^0.2.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
